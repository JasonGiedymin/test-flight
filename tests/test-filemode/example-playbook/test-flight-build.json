{
  "runTests": true,
  "owner": "testflight",
  "imageName": "test-docker-name1",
  "from": "jasongiedymin/ansible-nodejs",
  "version": "0.1.0",
  "env": [
    {"variable": "ENV_MODE", "value": "dev"}
  ],
  "ignore": [
    "ignoreme"
  ],
  "workDir": "/tmp/build",
  "add": {
    "simple": [
      "tests"
    ]
  },
  "expose": [3000, 3001],
  "cmd": "sleep 15 && apt-cache search apache2",
  "launchCmd": [
    "/bin/echo",
    "Hello Launch!"
  ],
  "resourceShare": {
    "mem": 1024,
    "cpu": 2
  }
}
